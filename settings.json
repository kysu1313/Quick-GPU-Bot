{
  // NOTE: Fields are case sensitive

  "settings": {
    // ========== DEBUG MODE ==============//
    // Use this if you need to tweak anything to prevent accidental purchases,
    // Learn from my mistakes...ðŸ˜†
    "DEBUG_MODE": "False",
    "debug": {
      "login_enabled": "True",

      // Set to 'False' if you want to test entering payment info
