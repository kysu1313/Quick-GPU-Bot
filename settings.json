      "purchase_enabled": "False",

      // Set to 'False' if you want to test the bot
      // without adding items to cart
      "test_payment_no_purchase": "True",
      "send_messages": "True"
    },

    // ========== BROWSER CONTROL ============= //

    // Browser selection and path (if requied)
    "use_firefox": "False",
    "use_firefox_profile": "True",
    // Replace '<your-username-here>' with your username
    "custom_firefox_exe_path": "C:\\Users\\<your-username-here>\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\al32vdl0.default-release",
    // If you want to use your browser profile, add the folder path here, otherwise leave it empty
    "use_chrome": "True",
    "use_chrome_profile": "False",
    // Replace '<your-username-here>' with your username
    "chrome_profile_path": "C:\\Users\\<your-username-here>\\AppData\\Local\\Google\\Chrome\\User Data\\",
    "custom_chrome_exe_path": "<your-chrome-driver-path-here>",
    // Loading images is slow
    "load_images": "False",

    // Loading a webpage is also slow. You can disable it here.
    // When using headless mode, if you have to enter a 2FA code for Newegg, a small popupbox will appear.
